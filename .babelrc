{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions"]
        },
        "debug": false
      }
    ],
    "babel-preset-react"
  ],
  "plugins": [
    "transform-runtime",
    "react-hot-loader/babel",
    "transform-decorators",
    "transform-object-rest-spread",
    "transform-decorators-legacy",
    "react-css-modules"
  ]
}